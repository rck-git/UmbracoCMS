@using Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Cms.Core.Models;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "mainLayout.cshtml";

	var successList = Model.Value<IEnumerable<string>>("successList");

	var projectList = Umbraco.Content(Guid.Parse("6b344d3a-8e6e-4683-83c4-f208ffc09d73"))
	.ChildrenOfType("all content")
	.Where(x => x.IsVisible());

}
<section class="hero">
	<div class="heroContainer">

		<div class="heroText">
			<div class="heroH1">
				<h1>
					@Model.Value("heroTitle")<br><span>@Model.Value("heroSpan")</span>@Model.Value("heroTitle2")
				</h1>
			</div>
			<p>
				@Model.Value("heroParagraph")
			</p>
			<a class="contactButton" href="/about">
				@Model.Value("contactButton")
			</a>
		</div>
		<img src="@Model.Value("heroImage")" alt="hero girl in glasses">
	</div>

</section>

<section class="about container">
	<div class="ceoDiv">
		<h6 class="sectionSubText">
			@Model.Value("aboutSmallTitle")
		</h6>
		<h2>
			@Model.Value("aboutTitle")
		</h2>
		<h5>
			@Model.Value("ceoQuoteParagraph")
		</h5>
		<p>
			@Model.Value("aboutParagraph")
		</p>

		<img src="@Model.Value("ceoSignatureImage")" alt="ceo signature">
		<p>
			@Model.Value("ceoNameParagraph")
		</p>
	</div>
	<div class="callbackForm">
		<div class="formBox">

			<div class="topBox">
				<h3>
					Request a call back
				</h3>
				
			</div>
			<editForm enctype="multipart/form-data" novalidate>

				<p><em>Dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur excepteur sint occaecat</em></p>

				<input type="text" id="name" placeholder="Name" />


				<input type="text" id="email" placeholder="Email address" />


				<input type="text" id="phone" placeholder="Phone" />


				<input type="text" id="consulting" placeholder="Financial Consulting" />


				<input type="submit" value="Submit" class="submitButton">@Model.Value("formSubmitButton")</input>
			</editForm>

		</div>

	</div>

</section>
<section class="ourServices">
	<h6 class="sectionSubText">@Model.Value("servicesSmallTitle")</h6>
	<h1>
		@Model.Value("servicesSectionTitle", fallback: Fallback.ToAncestors)
	</h1>
	@await Html.PartialAsync("static/ourServices")
</section>

<section class="success">
	<div class="container">
		<div class="trusted">
			<h2>
				@Model.Value("successTitle")
			</h2>
			<h5 class="sectionSubText">
				@Model.Value("successSubTitle")
			</h5>
			<p>
				@Model.Value("successParagraph")
			</p>
			<a class="btn" href="/about">
				@Model.Value("successButton")
			</a>
			<img id="successBg" src="@Model.Value("successBg")" class="colored-svg" alt="successful background">
		</div>

		<div class="successImages">

			<img id="successImage" src="@Model.Value("successImage")" alt="successful people">

		</div>
	</div>
	<div class="subSection">
		<h6 class="sectionSubText">
			@Model.Value("successSmallTitle")
		</h6>
		<h2>
			@Model.Value("successTitle2")
		</h2>
		<img id="successBg2" src="@Model.Value("successBg2")" alt="successful background">

		<div class="storyBoxes">
			<div class="storyBox">
				<span>@Model.Value("successStory1Span")</span>
				@Model.Value("successStory1")
			</div>
			<div class="storyBox">
				<span>@Model.Value("successStory2Span")</span>
				@Model.Value("successStory2")
			</div>
			<div class="storyBox">
				<span>@Model.Value("successStory3Span")</span>
				@Model.Value("successStory3")
			</div>
		</div>
	</div>
	<div class="successImages2">
		<img src="@Model.Value("successImage2")" id="successImage2" alt="People laughing">

	</div>


</section>
<section class="projects">
	<span class="sectionSubText">@Model.Value("projectsSmallTitle")</span>
	<h1>
		@Model.Value("projectsSectionTitle")
	</h1>

	@await Html.PartialAsync("static/recentProjects")
</section>
@await Html.PartialAsync("static/brands")

